<configuration>
	<!-- Console appender -->
	<appender name="CONSOLE_APPENDER" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<charset>UTF-8</charset>
			<pattern>[%d{HH:mm:ss.SSS}][%-5level][%thread][%logger{0}] %msg%n%ex
			</pattern>
		</encoder>
	</appender>

	<!-- Appender -->
	<root level="DEBUG">
		<appender-ref ref="CONSOLE_APPENDER" />
	</root>
</configuration>